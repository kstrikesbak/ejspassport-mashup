<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../partials/head')  %> 
        <title>Random Users</title>
    </head>
    <body>
        <div id = "body">
            <div id = "header">
                <header>Random Users</header>
            </div>
            <div id = "content">
                <% sortAlphabetically.forEach(({name: {first, last}, picture: {large}}) => {%> 
                    <div class="contentArea">
                        <img src=<%=large%>>
                        <p><%= first + ' ' + last %> </p>
                    </div>
                <%}) %>
            </div>
            <div id = "footer">
                <%- include('../partials/footer') %> 
            </div>
        </div>
    </body>
</html>